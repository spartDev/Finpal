{
  "presets": [
    ["latest"],
    "react",
    "stage-2"
  ],
  "plugins": [
    ["add-module-exports"],
    ["transform-runtime"]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "plugins": [
        "transform-class-properties",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "transform-react-remove-prop-types"
      ]
    },
  }
}
