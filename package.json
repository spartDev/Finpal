{
  "name": "watchit",
  "version": "1.0.0",
  "description": "Finpal test for qonto",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production PORT=8081 node ./dist/server.js",
    "start:dev": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "cross-env NODE_ENV=development babel-node ./webpack/middleware/development.server.babel.js",
    "watch:server": "cross-env NODE_ENV=development PORT=3000 nodemon . --exec babel-node",
    "build": "npm-run-all build:client build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.production.babel.js",
    "build:server": "cross-env NODE_ENV=production BABEL_ENV=server webpack --progress --colors --config ./webpack/webpack.config.server.babel.js",
    "lint": "npm-run-all --parallel lint:js lint:scss",
    "lint:js": "eslint src *.js",
    "lint:scss": "stylelint src/**/*.scss",
    "plato": "plato -r -e .eslintrc -d report -t 'Watch it' -x .test.js src/",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "test": "jest",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  },
  "bin": {
    "cross-env": "./node_modules/.bin/cross-env",
    "webpack": "./node_modules/.bin/webpack",
    "babel-node": "./node_modules/.bin/babel-node",
    "eslint": "./node_modules/.bin/eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png|ico)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/containers/index.js",
      "!src/redux/store.js",
      "!src/redux/reducers.js",
      "!src/client.js",
      "!src/server.js"
    ]
  },
  "engines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "keywords": [
    "React",
    "Redux",
    "node",
    "webpack",
    "watchit",
    "serverSide",
    "Universal",
    "Babel",
    "Alchimie",
    "Sass"
  ],
  "author": "Thomas Roux <roux.tom@gmail.com>",
  "license": "Private",
  "private": true,
  "dependencies": {
    "autoprefixer": "^6.7.6",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "https://registry.npmjs.org/babel-plugin-transform-class-properties/-/babel-plugin-transform-class-properties-6.23.0.tgz",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "https://registry.npmjs.org/babel-preset-stage-2/-/babel-preset-stage-2-6.22.0.tgz",
    "babel-runtime": "https://registry.npmjs.org/babel-runtime/-/babel-runtime-6.23.0.tgz",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.15",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "compression": "^1.6.2",
    "es6-promise": "^4.0.5",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "https://registry.npmjs.org/extract-text-webpack-plugin/-/extract-text-webpack-plugin-2.0.0.tgz",
    "file-loader": "^0.10.1",
    "helmet": "^3.4.1",
    "hpp": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "pretty-error": "^2.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
